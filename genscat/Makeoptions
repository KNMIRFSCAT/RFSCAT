# This Makeoptions file is ment to be included in every
# Makefile in the genscat tree, and in the application trees
# that use genscat
# This file is automatically generated
#
# A user may use one of the MY flags specified below to add
# his own options to each of these commands, or may leave
# them undefined

LINK       = f90
LINKFLAGS  = $(LIB) 
# the user may supply: MYLINKFLAGS

SHLINK       = f90
SHLINKFLAGS  = -shared $(LIB) 
# the user may supply: MYSHLINKFLAGS

CLINK       = cc
CLINKFLAGS  = $(LIB) 
# the user may supply: MYCLINKFLAGS

F90        = f90
F90FLAGS   = -O -fPIC 
# the user may supply: MYF90FLAGS

F77        = f90
F77FLAGS   = -O -fPIC 
# the user may supply: MYF77FLAGS

MODINCFLAG =  -I

HINCFLAG   =  -I

CPPDEF     =  -D

CMP_EXT    = generic

CC         = cc
CFLAGS     = -O -fPIC -DINTEGER_IS_INT 
# the user may supply: MYCFLAGS

RM         = rm
RMFLAGS    = -f 
# the user may supply: MYRMFLAGS

AR         = ar
ARFLAGS    = -rlcs 
# the user may supply: MYARFLAGS

LN         = ln
LNFLAGS    = -sf
# the user may supply: MYLNFLAGS

# Remove all default suffixes to avoid problems with implicit
# definitions and define the suffixes we use explicitly
.SUFFIXES:
.SUFFIXES: .f90 .F90 .f .F .c .o

# C compile command
.c.o:
	$(CC) $(CFLAGS) $(MYCFLAGS) $(INC) -c $< 

# Fortran90 compile command
.f90.o .F90.o:
	$(F90) $(F90FLAGS) $(MYF90FLAGS) $(INC) -c $<

# Fortran77 compile command
.f.o .F.o:
	$(F77) $(F77FLAGS) $(MYF77FLAGS) $(INC) -c $<
